      <page
            category="com.aptana.ui.AptanaPreferencePage"
            class="com.aptana.editor.common.preferences.TasksPreferencePage"
            id="com.aptana.editor.common.preferences.TasksPreferencePage"
            name="%page.tasks.name Tasks">
      </page>
      <page
            category="com.aptana.ui.AptanaPreferencePage"
