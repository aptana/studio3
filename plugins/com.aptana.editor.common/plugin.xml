       <user-agent
             icon="icons/safari_icon.png"
             icon-disabled="icons/safari_icon_grey.png"
             id="Safari"
             name="Safari">
       </user-agent>
       <user-agent
             icon="icons/chrome_icon.png"
             icon-disabled="icons/chrome_icon_grey.png"
